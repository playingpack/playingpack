{
  "name": "playingpack",
  "version": "0.1.0",
  "description": "Chrome DevTools for AI Agents - Local reverse proxy and debugger for LLM API calls",
  "type": "module",
  "bin": {
    "playingpack": "./dist/index.js"
  },
  "main": "./dist/config-helper.js",
  "types": "./dist/config-helper.d.ts",
  "exports": {
    ".": {
      "types": "./dist/config-helper.d.ts",
      "import": "./dist/config-helper.js"
    }
  },
  "files": [
    "dist",
    "public",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/playingpack/playingpack.git"
  },
  "homepage": "https://github.com/playingpack/playingpack#readme",
  "bugs": {
    "url": "https://github.com/playingpack/playingpack/issues"
  },
  "author": "Geoptly Intelligence Inc.",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "cd ../.. && pnpm run build:all"
  },
  "dependencies": {
    "jiti": "^2.4.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.1",
    "@fastify/websocket": "^10.0.1",
    "@trpc/server": "^10.45.0",
    "commander": "^12.0.0",
    "eventsource-parser": "^1.1.2",
    "fastify": "^4.26.0",
    "get-port": "^7.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "@vitest/coverage-v8": "^1.2.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "proxy",
    "debugger",
    "mock",
    "testing"
  ],
  "license": "BUSL-1.1"
}
